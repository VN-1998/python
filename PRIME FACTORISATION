def fact(n):
    for i in range(2,n+1):
        if n%i==0:
            print(i,end=" ")
            fact(n//i)
            break

n = int(input("enter the number:"))
fact(n)
or 
def fact(n):
    if(n==2):
        return
    i = 2
    while(n%i!=0):
        i = i+1
    print(i,end=" ")
    fact(n//i)
            

n = int(input("enter the number:"))
fact(n)
