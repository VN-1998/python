h = int(input("enter the number of head:"))
l = int(input("enter the number of legs"))
flag =False
for hen in range(h):
    cow = h-hen
    if (cow*4 + hen*2 == l):
        print("COWS:", cow)
        print("Hens:",hen)
        flag =True
        break
if (not flag):
    print("no solution")   
    
